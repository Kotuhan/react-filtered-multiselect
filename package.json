{
  "name": "react-filtered-multiselect",
  "description": "Filtered multi-select React component",
  "version": "0.4.0",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "homepage": "https://github.com/insin/react-filtered-multiselect",
  "standalone": "FilteredMultiSelect",
  "license": "MIT",
  "author": "Jonny Buchanan <jonathan.buchanan@gmail.com> (https://github.com/insin)",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/react-filtered-multiselect.js && webpack src/index.js dist/react-filtered-multiselect.min.js --config=webpack.prod.config.js",
    "dist": "npm run lint && npm run clean && npm run build && npm run build:umd",
    "demo": "eslint demo/src && rimraf demo/dist && webpack --config=webpack.demo.config.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "eslint ."
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=0.14.0"
  },
  "devDependencies": {
    "bootstrap": "3.3.5",
    "react": "0.14.0",
    "react-dom": "0.14.0",

    "babel": "5.8.29",
    "babel-core": "5.8.30",

    "eslint": "1.7.3",
    "babel-eslint": "4.1.3",
    "eslint-config-standard": "4.4.0",
    "eslint-config-standard-react": "1.1.0",
    "eslint-plugin-react": "3.5.3",
    "eslint-plugin-standard": "1.3.1",

    "webpack": "1.12.2",
    "babel-loader": "5.3.2",
    "css-loader": "0.21.0",
    "extract-text-webpack-plugin": "0.8.2",
    "file-loader": "0.8.4",
    "html-webpack-plugin": "1.6.2",
    "json-loader": "0.5.3",
    "style-loader": "0.13.0",

    "rimraf": "2.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/insin/react-filtered-multiselect.git"
  },
  "keywords": [
    "react",
    "react-component",
    "filtered",
    "multiselect",
    "select-multiple"
  ]
